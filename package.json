{
  "name": "phone-number-generator",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "server:dev": "babel-watch src/app.js",
    "build:server-prod": "babel src --out-dir dist",
    "start:server-prod": "node  dist/app.js",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha src/tests/index.test.js --require @babel/register --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build:client-dev": "rm -rf client/dist && webpack --env.mode development",
    "build:client-prod": "rm -rf client/dist && webpack --env.mode production",
    "client:dev": "rm -rf client/dist && webpack-dev-server --env.mode development --open --hot",
    "heroku-postbuild": "rimraf client/dist && mkdir client/dist && yarn build:server-prod && yarn build:client-prod"
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-loading": "^2.0.3",
    "react-router-dom": "^4.3.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "babel-watch": "https://github.com/monathur/babel-watch.git"
  }
}
